{
  "name": "share-list",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=v20.3.0",
    "pnpm": "8.6.2"
  },
  "scripts": {
    "fmt": "prettier --write .",
    "start": "esbuild src/index.tsx --bundle --target=es2022 --outfile=src/index.js --watch --servedir=src",
