    "ghp": "gh-pages -d src -b daily"
  },
  "devDependencies": {
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "esbuild": "0.18.4",
    "eslint": "8.43.0",
    "gh-pages": "5.0.0",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.13.0",
    "typescript": "5.1.3"
  }
}
